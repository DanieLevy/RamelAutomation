    "clear": "powershell \"Get-Process node -ErrorAction SilentlyContinue | Stop-Process -Force; Start-Sleep 2; if(Test-Path .next){Remove-Item .next -Recurse -Force}; npm cache clean --force; echo 'Project cleared! Run npm run dev to start fresh.'\"",
